<?xml version="1.0" encoding="utf-8" ?>
<ContentView xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:draw="clr-namespace:EBM2x.UI.Draw;assembly=EBM2x" 
             x:Class="EBM2x.UI.BackofficeEntry.DatePickerEntryPanel">
    <ContentView.Content>
        <draw:FixedGrid x:Name="entryFixedGrid" ColumnCount="1" RowCount="1">
            <draw:DrawBox Grid.Row="0" Grid.Column="0" Color="ffffff"/>
            <DatePicker x:Name="entryDatePicker" Grid.Row="0" Grid.Column="0"
                        MinimumDate="01/01/2010"
                        MaximumDate="12/31/2030"/>
        </draw:FixedGrid>
    </ContentView.Content>
</ContentView>